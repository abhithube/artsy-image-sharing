    "bcrypt": "^5.0.1",
    "compression": "^1.7.4",
    "connect-redis": "^6.1.3",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "express-graphql": "^0.12.0",
    "express-session": "^1.17.2",
    "graphql": "^15.8.0",
    "graphql-scalars": "^1.17.0",
    "graphql-tools": "^8.2.8",
    "helmet": "^5.0.2",
    "ioredis": "^5.0.4",
    "sharp": "^0.30.4"
